{
    "name": "jmf/grid-bundle",
    "description" : "Grid bundle for Symfony",
    "license": "mit",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.0",
        "symfony/property-access": "^6.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phing/phing": "^2.17",
        "phpmd/phpmd": "^2.10",
        "phpunit/phpunit": "^10.1",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Jmf\\Grid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmf\\Grid\\Tests\\": "tests"
        }
    }
}
